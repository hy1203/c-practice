using System;


class Cat
{
    public string Name;

    public Cat(string name)
    {
        Name = name;
        Console.WriteLine("고양이의 이름은 " +Name+"입니다.");
    }
}

class Program
{
    public static void Main(string[] args)
    {
        Cat myCat = new Cat("코코");
        Cat myCat2 = new Cat("몰리");
    }
}
