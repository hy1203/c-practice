using System;

class MainClass
{
    public static void Main(string[] args)
    {
        bool a, b;
        a = true;
        b = false;

        //논리 합
        Console.WriteLine(a && a);
        Console.WriteLine(a&&b);

        //논리 곱
        Console.WriteLine(a||b);
        Console.WriteLine(b||b);

        //부정
        Console.WriteLine(!a);
        Console.WriteLine(!b);
    }
}

