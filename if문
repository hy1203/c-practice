using System;

class MainClass
{
    public static void Main(string[] args)
    {
        int num = 10;

        if (num > 0)
        {
            Console.WriteLine("양수");
        }
        else if(num < 0)
        {
            Console.WriteLine("음수");
        }
        else
        {
            Console.WriteLine("영");
        }
    }
}

