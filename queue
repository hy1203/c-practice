using System;
using System.Collections;

public class MainClass
{
    public static void Main(string[] args)
    {
        Queue qu = new Queue(); //먼저 입력된 수가 먼저 빠져나옴

        //Enqueue메소드를 통해 아이템 추가
        qu.Enqueue(1);
        qu.Enqueue(2);
        qu.Enqueue(3);

        while(qu.Count > 0){
            Console.WriteLine(qu.Dequeue());
        }
    }
}
