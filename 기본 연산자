using System;

class MainClass
{
    public static void Main(string[] args)
    {
        int result, num1, num2;

        result = 3 + 1; //더하기
        Console.WriteLine(result);

        result = 3 - 1; //빼기
        Console.WriteLine(result);

        num1 = 2;
        result = 5 * num1; // 곱하기
        Console.WriteLine(result);

        num2 = 10;
        result = num2 / 3; // 나누기
        Console.WriteLine(result);

        result = 10 % 2; //나머지값
        Console.WriteLine(result);
    }
}

